# The names of the workers.
CELERY_FBRAT_NODES="rat_1 rat_2 rat_3 rat_4 rat_5 rat_6 rat_7 rat_8 rat_9 rat_10"

# The name of the Celery App, should be the same as the python file
# where the Celery tasks are defined
CELERY_APP="ratapi.tasks.afc_worker"

# Log and PID directories
CELERY_FBRAT_LOG_FILE="/var/log/celery/%n%I.log"
CELERY_FBRAT_PID_FILE="/var/run/celery/%n.pid"

# Log level
CELERY_FBRAT_LOG_LEVEL=INFO

# Path to celery binary, that is in your virtual environment
CELERY_BIN=/bin/celery
